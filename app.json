{
  "expo": {
    "jsEngine": "hermes",
    "name": "HostelApp",
    "slug": "hostelapp",
    "scheme": "mobile",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/hostelapp.png",
    "splash": {
      "backgroundColor": "#7f5de7",
      "resizeMode": "contain",
      "image": "./assets/images/logo/hostelapp.png"
    },
    "deepLinking": true,
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "jsEngine": "jsc",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "targetSdkVersion": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/hostelapp.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.marinspira.hostelapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/hostelapp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-apple-authentication"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow HostelApp to access your photos."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/hostelapp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6c63ff"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow HostelApp to access your Face ID biometric data."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "15ce5807-73d8-4f51-aa2b-2414b842f269"
      }
    },
    "owner": "marinspira",
    "updates": {
      "url": "https://u.expo.dev/15ce5807-73d8-4f51-aa2b-2414b842f269"
    }
  }
}
